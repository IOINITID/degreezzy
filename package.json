{
  "name": "degreezzy",
  "version": "2.0.0",
  "description": "Helper functions for working with degrees and directions.",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "package.json",
    "changelog.md",
    "readme.md"
  ],
  "scripts": {
    "dist-prebuild": "rm -rf ./dist",
    "dist-build": "webpack --progress",
    "dist-postbuild": "cp ./package.json ./dist && cp ./readme.md ./dist && cp ./changelog.md ./dist",
    "dist-pack": "npm pack ./dist",
    "dist-pack-check": "yarn run dist-pack --dry-run",
    "dist-prepublish": "yarn run dist-prebuild && yarn run dist-build && yarn run dist-postbuild",
    "dist-publish": "yarn run dist-prepublish && npm publish --registry=https://registry.npmjs.org ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IOINITID/degreezzy.git"
  },
  "keywords": [
    "Degreezzy, Degree, Direction, Navigation, Geo, Location, Radians"
  ],
  "author": "Igor Orishchenko <ioinitid@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IOINITID/degreezzy/issues"
  },
  "homepage": "https://github.com/IOINITID/degreezzy#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2"
  }
}
