{
  "name": "degreezzy",
  "version": "1.9.4",
  "description": "Helper functions for working with degrees and directions.",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "package.json",
    "changelog.md",
    "readme.md"
  ],
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "webpack --progress",
    "postbuild": "cp ./package.json ./dist && cp ./readme.md ./dist && cp ./changelog.md ./dist",
    "pack": "npm pack ./dist",
    "pack-check": "npm run pack ./dist --dry-run",
    "prepublish": "npm run prebuild && npm run build && npm run postbuild",
    "publish": "npm run prepublish && npm publish ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IOINITID/degreezzy.git"
  },
  "keywords": [
    "Degreezzy, Degree, Direction, Navigation, Geo, Location, Radians"
  ],
  "author": "Igor Orishchenko <ioinitid@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IOINITID/degreezzy/issues"
  },
  "homepage": "https://github.com/IOINITID/degreezzy#readme",
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-config-htmlacademy": "^0.6.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.1.2",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2"
  }
}
